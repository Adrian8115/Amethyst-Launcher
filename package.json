{
  "name": "amethyst-launcher",
  "version": "2.4.2",
  "main": "public/electron.js",
  "author": "FrederoxDev, Adrian8115",
  "description": "Amethyst Launcher",
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.70",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "electron-is-dev": "^3.0.1",
    "electron-updater": "^6.2.1",
    "jszip": "^3.10.1",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "regedit-rs": "^1.0.2",
    "sudo-prompt": "^9.2.1",
    "typescript": "^4.9.5",
    "util": "^0.12.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start-react": "react-app-rewired start",
    "start-electron": "electron .",
    "build-react": "react-app-rewired build",
    "build": "react-app-rewired build && electron-builder",
    "run-built": "start ./dist/win-unpacked/Amethyst-Launcher.exe",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.1"
  },
  "build": {
    "productName": "Amethyst-Launcher",
    "appId": "com.frederoxdev.amethystlauncher",
    "directories": {
      "output": "dist",
      "buildResources": "public"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/favicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "artifactName": "${productName}-${version}.exe"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "FrederoxDev",
        "repo": "Amethyst-Launcher"
      }
    ],
    "fileAssociations": [
      {
        "ext": [
          ".amethyst"
        ],
        "name": "Amethyst Mod",
        "role": "Editor"
      }
    ]
  },
  "browserslist": {
    "production": [
      "electron >= 28.1.3"
    ],
    "development": [
      "electron >= 28.1.3"
    ]
  },
  "homepage": "./"
}
